<% if LocalRigths.validate(Current.user.rights, "Address", "New") %>
$("#modal-window .modal-content").html("<%= escape_javascript(render "form", address: @address, remote: true) %>");
$("#modal-window").modal("show");
<% else %>
 <% flash[:alert] = "You are not permitted to view this page" %>
 location.replace("<%= request.referrer %>");
<% end %>
